#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_vitepress@1.0.0-alpha.15@vitepress/bin/vitepress.js" "$@"
  ret=$?
else 
  node  "$basedir/../_vitepress@1.0.0-alpha.15@vitepress/bin/vitepress.js" "$@"
  ret=$?
fi
exit $ret
